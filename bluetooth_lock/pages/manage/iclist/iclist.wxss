/* pages/manage/fingerprint/fingerprint.wxss */

.fingerprint {
  height: 114rpx;
  background-color: #303250;
  box-shadow: 0 8rpx 54rpx 0 rgba(9, 12, 51, 0.18);
  display: flex;
  align-items: center;
  padding: 0 57rpx;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}

.fingerprinttotal::after {
  content: "";
  padding: 0 95rpx 0 15rpx;
}

.addfingerprint::after {
  content: "";
  padding: 0 95rpx 0 15rpx;
}

.content {
  height: 114rpx;
  background-color: #303250;
  box-shadow: 0 8rpx 54rpx 0 rgba(9, 12, 51, 0.18);
  display: flex;
  align-items: center;
  padding: 0 57rpx;
  font-size: 30rpx;
  margin-bottom: 20rpx;
  justify-content: space-between;
}
.icon-zhiwen::after{
  padding-left: 15rpx;

}
.content_bnt{
	width: 133rpx;
	height: 48rpx;
	border-radius: 24rpx;
	border: solid 2rpx #aaaaaa;
  text-align: center;
  line-height: 48rpx;
}
.jionfingerprint{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
	background-color: #30324f;
  color: #aaa;
}
